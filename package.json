{
    "publisher": "jad-hamza",
    "name": "themedemolanguage",
    "displayName": "Theme Demo Language",
    "description": "A demo language highlighting",
    "version": "0.0.1",
    "repository": "https://github.com/Sporarum/CodeThemeDemo",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "themedemo",
                "aliases": [
                    "Theme Demo",
                    "ThemeDemo"
                ],
                "extensions": [
                    "tdm"
                ]
            }
        ],
        "grammars": [
            {
                "language": "themedemo",
                "scopeName": "source.tdm",
                "path": "./syntaxes/themeDemo.tmLanguage.json"
            }
        ]
    },
    "scripts": {
    },
    "devDependencies": {
        "@types/node": "^13.7.1",
        "@types/vscode": "^1.42.0"
    }
}
